{
  "expo": {
    "name": "Pro-Trainer",
    "slug": "pro-g",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "O app acessa suas imagens para atualizar o ser perfil. Gostaria de permitir esse uso ? "
        }
      ],
      [
        "expo-secure-store",
        {
          "faceIDPermission": "Para melhor proteger seus dados o app pode usar informações de Biometria como recurso de segurança. Gostaria de permitir esse uso ?"
        }
      ],
      "expo-font"
    ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.kayandesouzapereira.prog"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "150315c0-f552-431c-99f2-2cb167731b67"
      }
    },
    "owner": "kayandesouzapereira"
  }
}
